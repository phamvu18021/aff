<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_th_product_line_tree" model="ir.ui.view">
        <field name="name">th.product.line.tree</field>
        <field name="model">th.product.line</field>
        <field name="arch" type="xml">
            <tree string="list product line" editable="top">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="th_product_line_action" model="ir.actions.act_window">
        <field name="name">Dòng sản phẩm</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">th.product.line</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
